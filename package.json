    "prepare": "npm run build",
    "cp": "copyfiles -f src/client/img/* client/img",
    "build": "npm run cp && (tsc -p tsconfig-client.json || echo \"continue w/error\") && (tsc -p tsconfig-server.json  || echo \"continue w/error\")"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
